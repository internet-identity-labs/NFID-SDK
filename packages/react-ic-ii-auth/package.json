{
  "name": "@bendcircular/react-ic-ii-auth",
  "version": "0.0.11",
  "description": "React components to use DFINITYs Internet Identity Authentication",
  "author": "Philipp Litzenberger <philipp@bendcircular.com>",
  "license": "MIT",
  "repository": "bendcircular/react-ic-ii-auth",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "keywords": [
    "typescript",
    "react",
    "internet computer",
    "ic",
    "webauthn",
    "identity"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "commit": "cz",
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint . --ext .ts",
    "test:unit": "cross-env CI=1 react-scripts test --env=jest-environment-jsdom-sixteen",
    "test:watch": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "@dfinity/agent": "0.10.1",
    "@dfinity/auth-client": "0.10.1",
    "@dfinity/authentication": "0.10.1",
    "@dfinity/candid": "0.10.1",
    "@dfinity/identity": "0.10.1",
    "@dfinity/principal": "0.10.1",
    "react": ">=16.8.1",
    "react-dom": ">=16.8.1"
  },
  "devDependencies": {
    "@dfinity/agent": "0.10.0",
    "@dfinity/auth-client": "0.10.0",
    "@dfinity/authentication": "0.10.0",
    "@dfinity/candid": "0.10.0",
    "@dfinity/identity": "0.10.0",
    "@dfinity/principal": "0.10.1",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "^12.1.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.9.6",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^5.0.0",
    "gh-pages": "^3.2.3",
    "jest-environment-jsdom-sixteen": "2.0.0",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-scripts": "^3.4.1",
    "typescript": "^4.4.3"
  },
  "files": [
    "dist"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "moduleDirectories": [
    "node_modules",
    "src"
  ],
  "dependencies": {}
}
