{
  "version": "0.0.1-alpha.0",
  "name": "@internet-identity-labs/nfid-sdk-react",
  "repository": "https://github.com/internet-identity-labs/NFID-SDK",
  "author": "Philipp Litzenberger",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "@dfinity/agent": ">=0.10.2",
    "@identity-labs/react-ic-ii-auth": ">=0.0.16",
    "react": ">=16",
    "react-dom": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/tsdx-nfid-sdk-react.esm.js",
  "size-limit": [
    {
      "path": "dist/tsdx-nfid-sdk-react.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/tsdx-nfid-sdk-react.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@dfinity/agent": "^0.10.4",
    "@internet-identity-labs/nfid-ui-kit-core": "^0.0.1-alpha.0",
    "@identity-labs/react-ic-ii-auth": "0.0.16",
    "@rollup/plugin-image": "^2.1.1",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/addons": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@types/qrcode": "1.4.2",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/react-helmet": "6.1.5",
    "autoprefixer": "10.4.4",
    "babel-loader": "^8.2.4",
    "cssnano": "5.1.5",
    "eslint-config-standard": "16.0.3",
    "eslint-config-standard-react": "11.0.1",
    "husky": "^7.0.4",
    "identity-obj-proxy": "3.0.0",
    "postcss": "8.4.12",
    "react-is": "^17.0.2",
    "rollup-plugin-image-files": "1.4.2",
    "rollup-plugin-postcss": "4.0.2",
    "size-limit": "^7.0.8",
    "tailwindcss": "3.0.23",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "@dfinity/candid": "0.11.0",
    "@dfinity/principal": "0.11.0",
    "clsx": "1.1.1",
    "qrcode": "1.5.0",
    "react-helmet": "6.1.0",
    "react-icons": "4.3.1"
  }
}
