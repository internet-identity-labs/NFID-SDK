    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil/core": "2.15.0",
    "@types/jest": "^27.0.3",
    "jest": "^27.4.5",
    "jest-cli": "^27.4.5",
    "puppeteer": "^10.0.0"
  },
  "license": "MIT",
  "peerDependencies": {
    "@dfinity/auth-client": ">=0.10.0"
  }
