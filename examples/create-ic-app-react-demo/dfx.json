{
  "canisters": {
    "counter": {
      "type": "motoko",
      "main": "canister/counter/main.mo"
    },
    "internet_identity": {
      "type": "custom",
      "candid": "canister/internet_identity/internet_identity.did",
      "wasm": "canister/internet_identity/internet_identity_dev.wasm"
    },
    "identity_manager": {
      "type": "custom",
      "candid": "canister/nfid_backend/identity_manager.did",
      "wasm": "canister/nfid_backend/identity_manager.wasm"
    },
    "pub_sub_channel": {
      "type": "custom",
      "candid": "canister/nfid_backend/pub_sub_channel.did",
      "wasm": "canister/nfid_backend/pub_sub_channel.wasm"
    },
    "nfid_frontend": {
      "type": "assets",
      "source": ["canister/nfid_frontend/build"],
      "frontend": {
        "entrypoint": "canister/nfid_frontend/build/index.html"
      }
    },
    "demo_frontend": {
      "dependencies": ["counter"],
      "frontend": {
        "entrypoint": "build/index.html"
      },
      "source": ["build/"],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "dfx": "0.9.3",
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    },
    "ic": {
      "providers": ["https://mainnet.dfinity.network"],
      "type": "persistent"
    },
    "dev": {
      "providers": ["https://ic0.app"],
      "type": "persistent"
    }
  },
  "version": 1
}
