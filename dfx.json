{
  "canisters": {
    "internet_identity": {
      "type": "custom",
      "candid": "build/internet_identity/internet_identity.did",
      "wasm": "build/internet_identity/internet_identity.wasm.gz",
      "build": "curl -sSL https://github.com/dfinity/internet-identity/releases/download/release-2023-05-30/internet_identity_dev.wasm.gz -o build/internet_identity/internet_identity.wasm.gz"
    },
    "identity_manager": {
      "type": "custom",
      "candid": "build/identity_manager/identity_manager.did",
      "wasm": "build/identity_manager/identity_manager.wasm",
      "build": "echo use prebuild wasm"
    },
    "pub_sub_channel": {
      "type": "custom",
      "candid": "build/pub_sub_channel/pub_sub_channel.did",
      "wasm": "build/pub_sub_channel/pub_sub_channel.wasm",
      "build": "echo use prebuild wasm"
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    },
    "ic": {
      "providers": ["https://mainnet.dfinity.network"],
      "type": "persistent"
    },
    "dev": {
      "providers": ["https://ic0.app"],
      "type": "persistent"
    }
  },
  "version": 1
}
