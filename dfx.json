{
  "canisters": {
    "internet_identity": {
      "type": "custom",
      "candid": "src/internet_identity/internet_identity.did",
      "wasm": "src/internet_identity/internet_identity.wasm",
      "build": "curl -sSL https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity_dev.wasm -o src/internet_identity/internet_identity.wasm"
    },
    "identity_manager": {
      "type": "custom",
      "candid": "src/identity_manager/identity_manager.did",
      "wasm": "src/identity_manager/identity_manager.wasm"
    },
    "pub_sub_channel": {
      "type": "custom",
      "candid": "src/pub_sub_channel/pub_sub_channel.did",
      "wasm": "src/pub_sub_channel/pub_sub_channel.wasm"
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    },
    "ic": {
      "providers": ["https://mainnet.dfinity.network"],
      "type": "persistent"
    },
    "dev": {
      "providers": ["https://ic0.app"],
      "type": "persistent"
    }
  },
  "version": 1
}
